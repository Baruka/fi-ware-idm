<div id="organization">
  <header>
    <h3>
      <%= @organization.name %>
    </h3>

    <% if can? :edit, @organization.profile %>
      <%= link_to 'Edit', polymorphic_path([ @organization, :profile ], action: :edit), class: 'edit' %>
    <% end %>

    <%= link_to (image_tag(current_subject.logo.url(:medium) , :size => "small", :alt => current_subject.name )), current_subject  %>
  </header>

  <section class="personal_info">
    <div class="ocupation">
      <header>
        Contact us
      </header>
      <div class="result">
        <a href="#">wwww.oranization-site.com</a>
        </br></br>
        email@email.com
      </div>
    </div>
    <div class="accordion" id="accordion2">
      <div class="accordion-group">
        <div class="accordion-heading">
          <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseOne">
            Description
          </a>
        </div>
        <div id="collapseOne" class="accordion-body collapse">
          <div class="accordion-inner">
            <div class="content settings_content collapse in">
              <div class="result">
                organization.description organization.description organization.description
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <h4>
    Profile info
  </h4>
  <%= render partial: 'subjects/members',
             object: @organization,
             as: :subject %>

  <%= render partial: 'subjects/applications',
             object: @organization,
             as: :subject %>
</div>
