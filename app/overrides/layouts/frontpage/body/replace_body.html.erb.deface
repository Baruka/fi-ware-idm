          </div>
          <%= yield %>
        </div>
      </section>
      <br class="clearfloat" />
      
      <div class="push">
      </div>
    </div>
    <footer class="footerFront">
        <%= render :partial => "layouts/footer" %>
      </footer>
  </body>
