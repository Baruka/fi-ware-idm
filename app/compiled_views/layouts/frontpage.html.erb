<!DOCTYPE html>
<!-- HTML5 DOCTYPE --><html lang="en">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta charset="utf-8">
<title><%= yield(:title).empty? ? t('site.name') : yield(:title)%></title>
<meta name="keywords" content="<%= yield(:keywords).empty? ? t('profile.tags.default') : yield(:keywords) %>">
<meta name="description" content='<%= yield(:description).empty? ? "Home" : yield(:description) %>'>
<%= stylesheet_link_tag "application" %><%= javascript_include_tag "application" %><%= favicon_link_tag "icons/favicon.ico" %><% if protect_against_forgery? %><%= csrf_meta_tag %><% end %><script type="text/javascript">
    $(document).ready(function() {
      <%= yield :javascript%>
    });
    </script><%= yield :headers %>
</head>
  <body>
    <div class="wrapper container">
      <header id="header">
        <%= render :partial => "layouts/header" %>
      </header>
      <section>
        <div id="wrapper_body">
          <div id="flash">
            <%= render :partial => "layouts/flash", :locals => {:flash => flash} %>
          </div>
          
          <%= yield %>
        </div>
      </section>
      <br class="clearfloat">
      <div class="push">
      </div>
      <footer class="footerFront"><%= render :partial => "layouts/footer" %>
      </footer>
    </div>
  </body>
</html>
